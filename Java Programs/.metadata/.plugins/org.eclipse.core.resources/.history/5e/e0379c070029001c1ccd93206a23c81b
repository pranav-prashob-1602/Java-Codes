package com.pranav.exception;

import java.util.*;

public class Bank {

	
	public static void main(String[] args) {
		int balance=5000;
		Scanner sc = new Scanner(System.in);
		int wd = sc.nextInt();
		if(balance<wd) {
			throw new ArithmeticException();//ArithmeticException("Insufficient Balance!");
		}
		balance-=wd;
		System.out.println("Withdrawal amount : "+wd);
	}

}
