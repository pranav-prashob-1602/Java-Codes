package com.pranav.designPatterns.behavioural.mementoPattern;

public static  class Memento {
	final int savedAge;
	final String savedTime;
	
	Memento(int savedAge, String savedTime) {
		this.savedAge = savedAge;
		this.savedTime = savedTime;
	}
	
	int getSavedAge() {
		return savedAge;
	}
	
	String getSavedTime() {
		return savedTime;
	}
	
}
